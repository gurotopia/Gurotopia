            <Set name="Secondary_BL" UIName="Color - R,G,B"  UIDescription="Secondary Color:" DescriptionType="textbox" UIOrder="15" RadioGroup="-1" ForceFloats="1" MaxLengthForInput="13">255,73,255</Set>
